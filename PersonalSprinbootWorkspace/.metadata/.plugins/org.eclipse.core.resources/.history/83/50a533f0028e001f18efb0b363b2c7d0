package com.practice.dsa;

public class SelectionSort {
	public static void main(String[] args) {
		int[] arr = { 64, 34, 25, 12, 22, 11, 90 };
		System.out.println("Original array:");
		for (int i : arr) {
			System.out.print(i + " ");
		}
		int index=0;
		for(int i=0;i<arr.length-1;i++) {
			index=i;
			for(int j=i+1;j<i;j++) {
				if(arr[j]<arr[index]) {
					index=j;
				}
			}
            int temp = arr[Index];

		}
	}
}
