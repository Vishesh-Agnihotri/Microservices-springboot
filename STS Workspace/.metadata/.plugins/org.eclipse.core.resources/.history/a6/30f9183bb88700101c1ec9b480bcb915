		Player p13 = new Player("Milo", 1500, 10, "Red", true, "Sniper", 85, 1200, "NA", "Crossbow", true); // Duplicate of p1/p3
		Player p14 = new Player("Nia", 1300, 9, "Yellow", true, "Support", 80, 1100, "NA", "Pulse Rifle", true); // Duplicate of p9
		Player p15 = new Player("Orion", 800, 4, "Red", false, "Sniper", 60, 400, "ASIA", "Crossbow", false); // Duplicate of p10

		
		List<Player> onlinePlayers = players.stream()
			    .filter(Player::isOnline)
			    .collect(Collectors.toList());


